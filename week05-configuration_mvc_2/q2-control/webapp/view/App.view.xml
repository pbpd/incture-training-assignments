<mvc:View
	xmlns:core="sap.ui.core"
    controllerName="q2control.controller.App"
	xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
	xmlns="sap.m"
>
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<content>
						<ComboBox id="comboBox">
							<items>
								<core:Item text="Computer Science Engineering" id="1" />
								<core:Item text="Information Technology" id="2" />
								<core:Item text="Computer Engineering" id="3" />
								<core:Item text="Electronics and Telecommunications" id="4" />
								<core:Item text="Electrical and Electronics Engineering" id="5" />
							</items>
						</ComboBox>
						<Label id="lb" text="Enter Date" width="100%"></Label>
						<DatePicker id="datePicker" width="50%"></DatePicker>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>
