<mvc:View
    controllerName="q11icons.controller.App"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns:core="sap.ui.core"
    xmlns="sap.m"
>
    <Shell id="shell">
        <App id="app">
            <pages>
                <Page id="page" title="{i18n>title}">
                    <content>
                        <core:Icon id="icon" src="sap-icon://delete" size="100px"></core:Icon>
                    </content>
                </Page>
            </pages>
        </App>
    </Shell>
</mvc:View>
