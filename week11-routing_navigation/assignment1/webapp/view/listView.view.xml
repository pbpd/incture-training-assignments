<mvc:View
    controllerName="assignment1.controller.listView"
	xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
>
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page2" title="List View">
					<List id="empList" items="{JSON>/}">
						<items>
							<StandardListItem
                id="li"
				title="{JSON>Name}"
                info="{JSON>id}"
				description="{JSON>Department}"
                type="Navigation"
                press="handleNavigation"
			/>
						</items>
					</List>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>
