<mvc:View controllerName="io.pb.q3responsivescreen.controller.HomeView" xmlns:l="sap.ui.layout" xmlns:mvc="sap.ui.core.mvc" height="100%" width="100%" displayBlock="true" xmlns="sap.m">
    <Shell id="shell">
        <App id="app">
            <pages>
                <Page id="page" title="{i18n>title}">
                    <content>


                        <VBox alignItems="Center">
                            <IconTabBar id="_IDGenIconTabBar1" class="dtb">
                                <items>
    
                                    <IconTabFilter id="_IDGenIconTabFilter8" icon="sap-icon://account"></IconTabFilter>
                                    <IconTabFilter id="_IDGenIconTabFilter7" icon="sap-icon://home"></IconTabFilter>
                                    <IconTabFilter id="_IDGenIconTabFilter5" icon="sap-icon://search"></IconTabFilter>
    
                                </items>
                            </IconTabBar>
                            <Carousel id="_IDGenCarousel1" width="100%" height="35%" loop="true">
    
                                <Image id="_IDGenImage2" src="https://stimg.cardekho.com/images/carexteriorimages/930x620/Toyota/Land-Cruiser/8448/1623313668941/front-left-side-47.jpg" alt="c" />
                                <Image id="_IDGenImage3" src="https://stimg.cardekho.com/images/carexteriorimages/930x620/Toyota/Land-Cruiser/8448/1623313668941/interior-image-213.jpg" alt="c" />
                                <Image id="_IDGenImage4" src="https://stimg.cardekho.com/images/carexteriorimages/630x420/Mahindra/Thar/8076/1601635839903/front-left-side-47.jpg?tr=w-456"  alt="c"/>
                            </Carousel>
    
                            <HBox id="hbox1" class="hbox1" justifyContent="Start">
                                <Panel id="panel1" class="pnl1 pnlh">
                                    <Link id="btn2" text="View Service Status"></Link>
                                </Panel>
                            </HBox>
                            <l:VerticalLayout class="sapUiContentPadding" width="100%">
                            <Text text="Enter your feedback here:-" class="sapUiTinyMarginTop" />
                            <TextArea id="textAreaWithBinding2" value="{/value}" showExceededText="true" maxLength="40" width="100%" valueState="{= ${/value}.length > 40 ? 'Warning' : 'None' }" valueLiveUpdate="false" liveChange="handleLiveChange" />
                            <Button id="btn4" text="Submit your Feedback"></Button>
                        </l:VerticalLayout>
                        <Toolbar >
                            <Button type="Reject" icon="sap-icon://home" text="Home" ></Button>
                            <Button type="Reject" icon="sap-icon://car-rental" text="Vehicle"></Button>
                            <Button type="Reject" icon="sap-icon://product" text="Product"></Button>
                            <Button type="Reject" icon="sap-icon://overflow" text="More"></Button>
                            </Toolbar>
                        </VBox>
                        

                    </content>
                   
                </Page>
            </pages>
        </App>
    </Shell>
</mvc:View>